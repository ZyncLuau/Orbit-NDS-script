ModeCounter.Position = UDim2.new(0, 0, 0, 218)
ModeCounter.BackgroundTransparency = 1
ModeCounter.Text = "MODE: 1 / 6"
ModeCounter.TextColor3 = Color3.fromRGB(255, 255, 255)
ModeCounter.Font = Enum.Font.GothamBold
