local function createInput(name, yPos, default, color, callback)
    local label = Instance.new("TextLabel", MainFrame)
    label.Size = UDim2.new(1, 0, 0, 30)
    label.Position = UDim2.new(0, 0, 0, yPos)
    label.Text = name .. ": " .. default
    label.TextColor3 = color
    label.BackgroundTransparency = 1
    label.Font = Enum.Font.GothamBold
    label.TextSize = 16
    local box = Instance.new("TextBox", MainFrame)
    box.Size = UDim2.new(1, -40, 0, 30)
    box.Position = UDim2.new(0, 20, 0, yPos + 30)
    box.Text = tostring(default)
    box.BackgroundColor3 = Color3.fromRGB(40, 40, 55)
    box.TextColor3 = Color3.new(1, 1, 1)
    box.Font = Enum.Font.GothamBold
    box.TextSize = 14
    Instance.new("UICorner", box)
    box.FocusLost:Connect(function()
        local v = tonumber(box.Text)
        if v then label.Text = name..": "..v callback(v) end
    end)
end


createInput("SPEED", 250, ORBIT_SPEED, Color3.fromRGB(120, 255, 170), function(v) ORBIT_SPEED = v end)
createInput("RADIUS", 320, ORBIT_RADIUS, Color3.fromRGB(120, 220, 255), function(v) ORBIT_RADIUS = v end)


-- Interaction Connections
OrbitToggleBtn.MouseButton1Click:Connect(function()
    ORBIT_ACTIVE = not ORBIT_ACTIVE
    OrbitToggleBtn.Text = ORBIT_ACTIVE and "ORBIT: ON" or "ORBIT: OFF"
    OrbitToggleBtn.BackgroundColor3 = ORBIT_ACTIVE and Color3.fromRGB(60, 180, 100) or Color3.fromRGB(200, 60, 60)
    if not ORBIT_ACTIVE then
        for _, p in pairs(activeParts) do
            if p:FindFirstChild("OrbitBP") then p.OrbitBP:Destroy() end
            if p:FindFirstChild("OrbitBG") then p.OrbitBG:Destroy() end
        end
        activeParts = {}
    end
end)


ReleaseBtn.MouseButton1Click:Connect(function()
    if #activeParts == 0 then
        Message("System", "Energy empty!", 3)
        return
    end
    IS_CHARGING_KAMEHAMEHA = true
    ReleaseBtn.Text = "TARGET AND CLICK"
    ReleaseBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 0)
    Message("Kamehameha", "KA... ME... HA...", 4)
end)


UserInputService.InputBegan:Connect(function(input, gpe)
    if gpe then return end
    if input.UserInputType == Enum.UserInputType.MouseButton1 and IS_CHARGING_KAMEHAMEHA then
        fireKamehameha(Mouse.Hit.Position)
    end
    if input.KeyCode == Enum.KeyCode.P then toggleAntiFallDamage() end
end)


AxisBtn.MouseButton1Click:Connect(function()
    local currentIdx = table.find(MODES, ORBIT_AXIS) or 1
    local nextIdx = (currentIdx % #MODES) + 1
    ORBIT_AXIS = MODES[nextIdx]
    AxisBtn.Text = "AXIS: " .. ORBIT_AXIS:upper()
    ModeCounter.Text = "MODE: " .. nextIdx .. " / " .. #MODES
end)


-- // MAIN PHYSICS LOOP
RunService.Heartbeat:Connect(function()
    if not ORBIT_ACTIVE or not currentRoot or #activeParts == 0 then return end
    local t = tick() * ORBIT_SPEED
    local partCount = #activeParts
    for i, part in ipairs(activeParts) do
        if part and part.Parent and not part.Anchored then
            local bp = part:FindFirstChild("OrbitBP") or Instance.new("BodyPosition", part)
            bp.MaxForce = Vector3.one * math.huge
            bp.P = 50000
            bp.Name = "OrbitBP"
           
            local bg = part:FindFirstChild("OrbitBG") or Instance.new("BodyGyro", part)
            bg.MaxTorque = Vector3.one * math.huge
            bg.Name = "OrbitBG"
           
            local offset = Vector3.zero
            if ORBIT_AXIS == "Horizontal" then
                local angle = t + (i * (math.pi * 2 / partCount))
                offset = Vector3.new(math.cos(angle) * ORBIT_RADIUS, HEIGHT_OFFSET, math.sin(angle) * ORBIT_RADIUS)
            elseif ORBIT_AXIS == "Vertical" then
                local angle = t + (i * (math.pi * 2 / partCount))
                offset = Vector3.new(0, math.cos(angle) * ORBIT_RADIUS, math.sin(angle) * ORBIT_RADIUS)
            elseif ORBIT_AXIS == "Cross" then
                local angle = t + (i * (math.pi * 2 / partCount))
                offset = (i % 2 == 0) and Vector3.new(math.cos(angle) * ORBIT_RADIUS, HEIGHT_OFFSET, math.sin(angle) * ORBIT_RADIUS) or Vector3.new(0, math.cos(angle) * ORBIT_RADIUS, math.sin(angle) * ORBIT_RADIUS)
            elseif ORBIT_AXIS == "Sphere" then
                local phi = math.acos(1 - 2 * (i / partCount))
                local theta = math.sqrt(partCount * math.pi) * phi + t
                offset = Vector3.new(math.cos(theta) * math.sin(phi), math.sin(theta) * math.sin(phi), math.cos(phi)) * ORBIT_RADIUS
            elseif ORBIT_AXIS == "Spiral" then
                local angle = t + (i * (math.pi * 2 / 10))
                offset = Vector3.new(math.cos(angle) * ORBIT_RADIUS, math.sin(t + (i * 0.5)) * ORBIT_RADIUS, math.sin(angle) * ORBIT_RADIUS)
            elseif ORBIT_AXIS == "Dome" then
                -- // UPDATED DOME PHYSICS (Centered Fibonacci Sphere)
                local goldenRatio = (1 + math.sqrt(5)) / 2
                local y = 1 - (i / (partCount - 1)) * 2 -- Centered around 0
                local radiusAtY = math.sqrt(1 - y * y)
                local theta = 2 * math.pi * i / goldenRatio + t
               
                offset = Vector3.new(
                    math.cos(theta) * radiusAtY * ORBIT_RADIUS,
                    y * ORBIT_RADIUS + HEIGHT_OFFSET, -- Centered and lifted by offset
                    math.sin(theta) * radiusAtY * ORBIT_RADIUS
                )
            end
           
            bp.Position = currentRoot.CFrame:PointToWorldSpace(offset)
            bg.CFrame = CFrame.lookAt(part.Position, currentRoot.Position)
        else
            table.remove(activeParts, i)
        end
    end
end)

