-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local StarterGui = game:GetService("StarterGui")


-- Shared Variables
local Player = Players.LocalPlayer
local Mouse = Player:GetMouse()
local Character = Player.Character or Player.CharacterAdded:Wait()
local currentRoot = Character:WaitForChild("HumanoidRootPart")


-- State Variables
local antiFallEnabled = false
local antiFallConnection = nil
local characterAddedConnection = nil


local ORBIT_ACTIVE = false
local ORBIT_SPEED = 15
local ORBIT_RADIUS = 20
local HEIGHT_OFFSET = 3
local ORBIT_AXIS = "Horizontal"
local activeParts = {}
local SCAN_RATE = 1.5
local MODES = {"Horizontal", "Vertical", "Cross", "Sphere", "Spiral", "Dome"}


-- Selection State
local IS_CHARGING_KAMEHAMEHA = false
local THROW_VELOCITY = 500


-- Persistence
local function onCharacterAdded(char)
    Character = char
    currentRoot = char:WaitForChild("HumanoidRootPart")
end
Player.CharacterAdded:Connect(onCharacterAdded)


-- Notification
local function Message(_Title, _Text, Time)
    StarterGui:SetCore("SendNotification", {
        Title = _Title,
        Text = _Text,
        Duration = Time
    })
end


-- // STRAIGHT-LINE KAMEHAMEHA FIRE LOGIC
local function fireKamehameha(targetPos)
    IS_CHARGING_KAMEHAMEHA = false
    ORBIT_ACTIVE = false
   
    local mainFrame = Player.PlayerGui:FindFirstChild("OrbitUtility2026"):FindFirstChild("MainFrame")
    local orbitBtn = mainFrame:FindFirstChild("OrbitBtn")
    local releaseBtn = mainFrame:FindFirstChild("ReleaseBtn")
   
    if orbitBtn then
        orbitBtn.Text = "ORBIT: OFF"
        orbitBtn.BackgroundColor3 = Color3.fromRGB(200, 60, 60)
    end
    if releaseBtn then
        releaseBtn.Text = "KAMEHAMEHA RELEASE"
        releaseBtn.BackgroundColor3 = Color3.fromRGB(0, 160, 255)
    end


    Message("Kamehameha", "ME... HA!", 2)


    for _, part in ipairs(activeParts) do
        if part and part.Parent and not part.Anchored then
            if part:FindFirstChild("OrbitBP") then part.OrbitBP:Destroy() end
            if part:FindFirstChild("OrbitBG") then part.OrbitBG:Destroy() end
           
            local direction = (targetPos - part.Position).Unit
            local bv = Instance.new("BodyVelocity")
            bv.Name = "KamehamehaVelocity"
            bv.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            bv.Velocity = direction * THROW_VELOCITY
            bv.Parent = part
           
            game:GetService("Debris"):AddItem(bv, 3)
        end
    end
    activeParts = {}
end


-- Anti-Fall Logic
local function toggleAntiFallDamage()
    antiFallEnabled = not antiFallEnabled
    if antiFallEnabled then
        if game.PlaceId ~= 189707 then
            Message("Error", "Not Natural Disasters Survival!", 5)
            antiFallEnabled = false
            return
        end
        local hb = RunService.Heartbeat
        local rsd = RunService.RenderStepped
        local function protect(char)
            local root = char:WaitForChild("HumanoidRootPart")
            if root then
                local con
                con = hb:Connect(function()
                    if not antiFallEnabled or not root.Parent then con:Disconnect() return end
                    local vel = root.AssemblyLinearVelocity
                    root.AssemblyLinearVelocity = Vector3.zero
                    rsd:Wait()
                    root.AssemblyLinearVelocity = vel
                end)
                antiFallConnection = con
            end
        end
        protect(Player.Character)
        characterAddedConnection = Player.CharacterAdded:Connect(protect)
        Message("AntiFall", "Enabled", 3)
    else
        if antiFallConnection then antiFallConnection:Disconnect() end
        if characterAddedConnection then characterAddedConnection:Disconnect() end
        Message("AntiFall", "Disabled", 3)
    end
end


-- Snatch Logic
local function claimPart(part)
    if part:IsA("BasePart") and not part.Anchored and not part:IsDescendantOf(Character) then
        part.Velocity = Vector3.new(0, 0.1, 0)
        if part.Transparency < 0.25 then part.Transparency = 0.26 end
        return true
    end
    return false
end


-- Background Scanner
task.spawn(function()
    while true do
        if ORBIT_ACTIVE then
            for _, v in ipairs(workspace:GetDescendants()) do
                if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(Character) then
                    if not table.find(activeParts, v) then
                        if claimPart(v) then table.insert(activeParts, v) end
                    end
                end
            end
        end
        task.wait(SCAN_RATE)
    end
end)


-- UI SETUP
local ScreenGui = Instance.new("ScreenGui", Player.PlayerGui)
ScreenGui.Name = "OrbitUtility2026"
ScreenGui.ResetOnSpawn = false


local MainFrame = Instance.new("Frame", ScreenGui)
MainFrame.Name = "MainFrame"
MainFrame.Size = UDim2.new(0, 260, 0, 480)
MainFrame.Position = UDim2.new(1, -300, 0.5, -240)
MainFrame.BackgroundColor3 = Color3.fromRGB(15, 15, 20)
MainFrame.BackgroundTransparency = 0.1
Instance.new("UICorner", MainFrame).CornerRadius = UDim.new(0, 15)
Instance.new("UIStroke", MainFrame).Color = Color3.fromRGB(60, 60, 80)


local TitleBar = Instance.new("TextLabel", MainFrame)
TitleBar.Size = UDim2.new(1, 0, 0, 45)
TitleBar.BackgroundColor3 = Color3.fromRGB(45, 45, 75)
TitleBar.Text = "ORBIT CONTROL 2026"
TitleBar.TextColor3 = Color3.new(1, 1, 1)
TitleBar.Font = Enum.Font.GothamBold
TitleBar.TextSize = 16
Instance.new("UICorner", TitleBar).CornerRadius = UDim.new(0, 15)


-- Draggable UI
local dragging, dragStart, startPos
TitleBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true dragStart = input.Position startPos = MainFrame.Position
    end
end)
UserInputService.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - dragStart
        MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)
UserInputService.InputEnded:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end end)


local OrbitToggleBtn = Instance.new("TextButton", MainFrame)
OrbitToggleBtn.Name = "OrbitBtn"
OrbitToggleBtn.Size = UDim2.new(1, -30, 0, 50)
OrbitToggleBtn.Position = UDim2.new(0, 15, 0, 60)
OrbitToggleBtn.Text = "ORBIT: OFF"
